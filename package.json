{
    "name": "alloy",
    "displayName": "Alloy",
    "description": "support for the Alloy modelling language",
    "version": "0.2.1",
    "publisher": "ArashSahebolamri",
    "repository": {
        "type": "git",
        "url": "https://github.com/s-arash/org.alloytools.alloy/tree/ls"
    },
    "engines": {
        "vscode": "^1.23.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:alloy"
    ],
    "main": "./out/extension",
    "contributes": {
        "languages": [
            {
                "id": "alloy",
                "aliases": [
                    "Alloy",
                    "alloy"
                ],
                "extensions": [
                    ".als"
                ]
            }
        ],
        "configuration": [
			{
				"title": "Alloy",
				"properties": {
					"alloy.commandHighlightMode": {
                        "type": "string",
                        "enum": [
                            "codelens",
                            "link"
                        ],
						"default": "codelens",
						"description": "How run and check commands should be highlighted in code"
					}
				}
			}
		],
        "commands": [
            {
                "command": "alloy.openAlloyEditor",
                "title": "Open Alloy Editor"
            },
            {
                "command": "alloy.listCommands",
                "title": "List Alloy Commands"
            },
            {
                "command": "alloy.executeAllCommands",
                "title": "Execute All Alloy Commands"
            },
            {
                "command": "alloy.openLatestInstance",
                "title": "Open Latest Alloy Instance"
            }
        ]
    },
    "keywords": [
        "Alloy"
    ],
    "preview": true,
    "extensionDependencies": [
        "DongyuZhao.alloy-vscode"
    ],
    "author": {
        "name": "Arash Sahebolamri"
    },
    "qna": "marketplace",
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.18",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "vscode-languageclient": "^4.3.0"
    }
}
